generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ApiLog {
  id             Int       @id @default(autoincrement())
  api_id         String?   @db.VarChar(50)
  sandbox_id     String?   @db.VarChar(50)
  user_id        String?   @db.VarChar(50)
  status         String?   @db.VarChar(20)
  operation_type String?   @db.VarChar(50)
  description    String?
  added_at       DateTime? @default(now()) @db.Timestamptz(6)

  @@map("api_logs")
}

model DocumentLog {
  id             Int       @id @default(autoincrement())
  user_id        String?   @db.VarChar(50)
  sandbox_id     String?   @db.VarChar(50)
  name           String?   @db.VarChar(100)
  status         String?   @db.VarChar(20)
  operation_type String?   @db.VarChar(50)
  accessed_at    DateTime? @default(now()) @db.Timestamptz(6)

   @@map("document_logs")
}

model Membership {
  id           Int       @id @default(autoincrement())
  member_id    String?   @db.VarChar(100) @map("memberId")
  email        String?   @db.VarChar(100)
  first_name   String?   @db.VarChar(100) @map("firstName")
  last_name    String?   @db.VarChar(100) @map("lastName")
  phone_number String?   @db.VarChar(100) @map("phoneNumber")
  userid       String?   @db.VarChar(100)
  created_at   DateTime? @default(now()) @db.Timestamptz(6) @map("createdAt")
  updated_at   DateTime? @default(now()) @db.Timestamptz(6) @map("updatedAt")
  acces_code   String?   @db.VarChar(100) @map("accessCode")
  interest     String?   @db.VarChar(100)
  age_range    String?   @db.VarChar(100) @map("ageRange")
  location     String?   @db.VarChar(100)
  company_name String?   @db.VarChar(100) @map("companyName")
  specialty    String?   @db.VarChar(100)
  gender       String?   @db.VarChar
  facebook_id  String?   @db.VarChar(100) @map("facebookId")
  twitter_id   String?   @db.VarChar(100) @map("twitterId")
  linkedin_id  String?   @db.VarChar(100) @map("linkedinID")
  instagram_id String?   @db.VarChar(100) @map("instagramId")

   @@map("memberships")
}
